{
 "env": {
		"browser": true,
		"es2021": true,
		"node": true
 },
 "extends": [
		 "plugin:prettier/recommended"
 ],
 "parser": "@typescript-eslint/parser",
 "parserOptions": {
		 "ecmaVersion": 12,
		 "sourceType": "module"
 },
 "plugins": [
		 "@typescript-eslint"
 ],
 "rules": {
	"no-mixed-spaces-and-tabs": "off",
	"valid-jsdoc": "off",
	"no-tabs": "off",
	"guard-for-in": "off",
	"indent": "off"
 }
}
