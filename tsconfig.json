{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "./build",
    "strict": true,
    "moduleResolution": "node",
    "typeRoots": ["./node_modules/@types", "./types"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
	"include": ["./types/**/*.d.ts"]
}
